{
    "name": "omnifraud/signifyd",
    "description": "Signifyd driver for the Omnifraud PHP fraud prevention library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "LXRandCo",
            "email": "hugo.v@lxrco.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../../common"
        }
    ],
    "require": {
        "omnifraud/common": "dev-master",
        "signifyd/signifyd-php": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Omnifraud\\Signifyd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
